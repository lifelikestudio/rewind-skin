<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="canonical" href="{{ canonical_url }}">
    <link
      rel="preconnect"
      href="https://cdn.shopify.com"
      crossorigin>
    {%- unless settings.type_header_font.system? and settings.type_body_font.system? -%}
      <link
        rel="preconnect"
        href="https://fonts.shopifycdn.com"
        crossorigin>
    {%- endunless -%}
    <title>Rewind Skin</title>
    {{ content_for_header }}
    {% if template contains 'customers' %}
      <script src="{{ 'shopify_common.js' | shopify_asset_url }}" defer></script>
      <script src="{{ 'customer_area.js' | shopify_asset_url }}" defer></script>
    {% endif %}
    {{ 'global.css' | asset_url | stylesheet_tag }}
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.6.3/dist/jquery.min.js" defer></script>

    <script src="//embed.typeform.com/next/embed.js" defer></script>
    <script src="{{ 'global.js' | asset_url }}" defer></script>
    <link rel="dns-prefetch" href="//typeform.com">
    <style>
      .template-index .hero {
        background-image: url('{{ 'home_hero_desktop_LNK7977_hi-res_compressed.jpg' | asset_url }}');
      }
      @media (max-width: 1055px) {
        .template-index .hero {
          background-image: url('{{ 'home_hero_tablet_LNK7977_hi-res_compressed.jpg' | asset_url }}');
        }
      }
      @media (max-width: 671px) {
        .template-index .hero {
          background-image: url('{{ 'home_hero_mobile_LNK7977_hi-res_compressed.jpg' | asset_url }}');
        }
      }
      .template-collection .hero {
        background-image: url('{{ 'treatments_skin_hero_LNK7841_01_1784-greater_low-res_compressed.jpg' | asset_url }}');
      }
      .template-list-collections .hero {
        background-image: url('{{ 'treatments_skin_hero_LNK7841_01_1784-greater_low-res_compressed.jpg' | asset_url }}');
      }
      .skin-treatments .hero {
        background-image: url('{{ 'treatments_skin_hero_LNK7841_01_1784-greater_low-res_compressed.jpg' | asset_url }}');
      }
      .face-treatments .hero {
        background-image: url('{{ 'treatments_face_hero_LNK7825_01_low-res_compressed.jpg' | asset_url }}');
      }
      .body-treatments .hero {
        background-image: url('{{ 'treatments_body_hero_dts_01_low-res_compressed.jpg' | asset_url }}');
      }
      #shopify-section-opt-in {
        background-image: url('{{ 'collections_opt-in_LNK8624_01_ar-2-1_1784-greater_low-res_compressed.jpg' | asset_url }}');
      }
      .template-index #shopify-section-opt-in {
        background-image: url('{{ 'global_opt-in_desktop_22_1004_RewindSkin6570_hi-res_compressed.jpg' | asset_url }}');
      }

      @media (max-width: 1055px) {
        .template-index #shopify-section-opt-in {
          background-image: url('{{ 'global_opt-in_tablet_22_1004_RewindSkin6570_hi-res_compressed.jpg' | asset_url }}');
        }
      }

      @media (max-width: 671px) {
        .template-index #shopify-section-opt-in {
          background-image: url('{{ 'global_opt-in_mobile_22_1004_RewindSkin6570_hi-res_compressed.jpg' | asset_url }}');
        }
      }
      .treatments-category__cta .treatments-category__cta-container:first-child {
        background-image: url('{{ 'treatments_shop-skincare_LNK39_01_low-res_compressed.jpg' | asset_url }}');
      }
      .treatments-category__cta .treatments-category__cta-container:last-child {
        background-image: url('{{ 'treatments_shop-gifts_LNK7709_01_low-res_compressed.jpg' | asset_url }}');
      }

      .skin-analysis-section__container {
        background-image: url('{{'' | asset_url}}')
      }
    </style>
  </head>
  {% liquid
    if template.suffix
      assign current_template = template.suffix
    else
      assign current_template = template.name
    endif

    assign bodyClass = 'template-' | append: current_template

    if page.title
      assign pageTitle = page.title | handle
      assign bodyClass = bodyClass | append: ' ' | append: pageTitle
    endif
  %}

  <body data-customer-logged-in="{% if customer %}true{% else %}false{% endif %}" class="{{ bodyClass }}">
    {% if template contains 'product' or template contains 'cart' or template contains 'search' or template contains 'login' or template contains 'register' or template contains 'activate_account' or template contains 'reset_password' or template contains 'account' or template contains 'order' or template contains 'addresses'
    %}
      <section class="announcement-bar">
        <div class="wrapper wrapper--lg announcement-bar__container">
          {% comment %}
            <a
            class="announcement-bar__type"
            href="https://app.acuityscheduling.com/catalog.php?owner=18442968&category=PROMO+FACIALS"
            target="_blank"
            id="annual-facials-sale"
            >Buy 2 Facials, Get 1 Free. On Now Until September 8th. Book Here.</a
            >
          {% endcomment %}
          <a
            class="announcement-bar__type"
            href="/collections/all"
            id="evergreen-announcement">Free Shipping on Orders Over $149 CAD. Start Here.</a>
        </div>
      </section>
      <header>
